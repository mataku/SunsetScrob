{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^buildSrc/src/main/kotlin/Deps\\.kt$"],
      "matchStrings": [".*?const val [a-zA-Z0-9]+ = \"(?<groupName>.*?):(?<depName>.*?):(?<currentValue>)\"\n$"],
      "datasourceTemplate": "maven",
      "depNameTemplate": "gradle"
    }
  ]
}
